<?xml version='1.0' encoding='UTF-8'?>
<data xmlns="urn:ietf:params:xml:ns:netconf:base:1.0">
  <ptp xmlns="urn:ieee:std:1588:yang:gptp-yangconfig">
    <instance-domain-map>0x0000</instance-domain-map>
    <instances>
      <instance>
        <instance-index>0</instance-index>
        <default-ds>
          <priority1>248</priority1>
          <priority2>248</priority2>
          <external-port-config-enable>false</external-port-config-enable>
          <time-source>internal-oscillator</time-source>
	  <ptp-timescale>true</ptp-timescale>
	  <clock-quality>
	    <clock-class>cc-default</clock-class>
	    <clock-accuracy>ca-time-accurate-to-250-ns</clock-accuracy>
	    <offset-scaled-log-variance>0x436a</offset-scaled-log-variance>
	  </clock-quality>
        </default-ds>
        <ports>
          <port>
            <port-index>1</port-index>
	    <underlying-interface>tilld0</underlying-interface>
	    <port-ds>
	      <port-enable>true</port-enable>
	      <log-announce-interval>0</log-announce-interval>
	      <gptp-cap-receipt-timeout>3</gptp-cap-receipt-timeout>
	      <announce-receipt-timeout>3</announce-receipt-timeout>
	      <log-sync-interval>-3</log-sync-interval>
	      <minor-version-number>1</minor-version-number>
	      <initial-log-announce-interval>0</initial-log-announce-interval>
	      <sync-receipt-timeout>3</sync-receipt-timeout>
	      <initial-log-sync-interval>-3</initial-log-sync-interval>
	      <current-log-sync-interval>-3</current-log-sync-interval>
	      <current-log-gptp-cap-interval>3</current-log-gptp-cap-interval>
	      <initial-log-pdelay-req-interval>0</initial-log-pdelay-req-interval>
	      <current-log-pdelay-req-interval>0</current-log-pdelay-req-interval>
	      <allowed-lost-responses>9</allowed-lost-responses>
	      <allowed-faults>9</allowed-faults>
	      <mean-link-delay-thresh>0x27100000</mean-link-delay-thresh>
	    </port-ds>
          </port>
        </ports>
      </instance>
    </instances>
  </ptp>
</data>
